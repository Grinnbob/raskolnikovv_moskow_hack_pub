{
    "name": "raskolnikovv_be",
    "version": "0.1.0",
    "description": "",
    "author": "GRIFON",
    "private": true,
    "license": "GRIFON_INCORPORATED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=prod nest start",
        "start:dev": "cross-env NODE_ENV=dev nest start --watch | npx pino-pretty",
        "start:docker": "cross-env NODE_ENV=docker nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "db:migration:create": "npx sequelize-cli migration:generate --name new-migration",
        "db:seed:create": "npx sequelize-cli seed:generate --name new-seed",
        "db:migrations:dev": "cross-env NODE_ENV=dev npx sequelize-cli db:migrate",
        "db:seeds:dev": "cross-env NODE_ENV=dev npx sequelize-cli db:seed:all",
        "db:migrationAndSeed:dev": "cross-env NODE_ENV=dev npx sequelize-cli db:migrate && cross-env NODE_ENV=dev npx sequelize-cli db:seed:all",
        "db:reset:dev": "cross-env NODE_ENV=dev npx sequelize-cli db:drop && cross-env NODE_ENV=dev npx sequelize-cli db:migrate && cross-env NODE_ENV=dev npx sequelize-cli db:seed:all",
        "db:migrationAndSeed:docker": "cross-env NODE_ENV=docker npx sequelize-cli db:migrate && cross-env NODE_ENV=docker npx sequelize-cli db:seed:all"
    },
    "dependencies": {
        "@nestjs/common": "^6.0.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^6.0.0",
        "@nestjs/jwt": "^6.0.0",
        "@nestjs/platform-express": "^6.0.0",
        "@nestjs/sequelize": "^6.0.0",
        "@nestjs/serve-static": "^3.0.0",
        "@nestjs/swagger": "^6.1.2",
        "@nestjs/throttler": "^5.0.0",
        "@types/pino": "^7.0.5",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cross-env": "^7.0.3",
        "helmet": "^7.0.0",
        "html-to-docx": "^1.8.0",
        "moment": "^2.29.4",
        "nestjs-pino": "^3.5.0",
        "nodemailer": "^6.9.7",
        "pdf-creator-node": "^2.3.5",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "pino": "^8.16.1",
        "pino-http": "^8.5.1",
        "pm2": "^5.3.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "sequelize": "^6.25.2",
        "sequelize-typescript": "^2.1.4",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^6.0.0",
        "@nestjs/schematics": "^6.0.0",
        "@nestjs/testing": "^6.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.8",
        "@types/multer": "^1.4.8",
        "@types/node": "^16.0.0",
        "@types/sequelize": "^4.28.14",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "28.1.3",
        "pino-pretty": "^10.2.3",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.8",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.1.0",
        "typescript": "^4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
